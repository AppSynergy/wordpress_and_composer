{
  "name": "root/composewp",
  "authors": [{
    "name": "Adam Marshall",
    "email": "adam@appsynergy.net"
  }],
  "repositories": [{
    "type": "composer",
    "url": "https://wpackagist.org"
  }],
  "require": {
    "php": ">=5.4",
    "johnpbloch/wordpress": "4.4.2",
    "wpackagist-plugin/captcha": "4.1.9",
    "wpackagist-theme/greenr": "1.1.4"
  },
  "extra": {
    "wordpress-install-dir": "public/wp",
    "installer-paths": {
      "public/content/themes/{$name}": ["type:wordpress-theme"],
      "public/content/plugins/{$name}": ["type:wordpress-plugin"]
    }
  }
}
